{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azure-project-hithesh-adf"
		},
		"ls_AzureBlobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureBlobstorage'"
		},
		"ls_Azure_database_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_Azure_database'"
		},
		"ls_Azure_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_Azure_databricks'"
		},
		"ls_Onprem_Sqlserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_Onprem_Sqlserver'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://Azure-Project-db-KV.vault.azure.net/"
		},
		"ls_Azure_database_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azure-project-azserver.database.windows.net"
		},
		"ls_Azure_database_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azure-project-azdatabase"
		},
		"ls_Azure_database_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"ls_Azure_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0321-124048-gz4eghf1"
		},
		"ls_Onprem_Sqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "CHANDU"
		},
		"ls_Onprem_Sqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Sample"
		},
		"ls_Onprem_Sqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azure-project-hithesh-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureBlobstorage_connectionString": {
						"value": "[parameters('ls_AzureBlobstorage_connectionString')]"
					},
					"ls_Azure_database_password": {
						"value": "[parameters('ls_Azure_database_password')]"
					},
					"ls_Azure_databricks_accessToken": {
						"value": "[parameters('ls_Azure_databricks_accessToken')]"
					},
					"ls_Onprem_Sqlserver_password": {
						"value": "[parameters('ls_Onprem_Sqlserver_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"ls_Azure_database_properties_typeProperties_server": {
						"value": "[parameters('ls_Azure_database_properties_typeProperties_server')]"
					},
					"ls_Azure_database_properties_typeProperties_database": {
						"value": "[parameters('ls_Azure_database_properties_typeProperties_database')]"
					},
					"ls_Azure_database_properties_typeProperties_userName": {
						"value": "[parameters('ls_Azure_database_properties_typeProperties_userName')]"
					},
					"ls_Azure_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_Azure_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_Onprem_Sqlserver_properties_typeProperties_server": {
						"value": "[parameters('ls_Onprem_Sqlserver_properties_typeProperties_server')]"
					},
					"ls_Onprem_Sqlserver_properties_typeProperties_database": {
						"value": "[parameters('ls_Onprem_Sqlserver_properties_typeProperties_database')]"
					},
					"ls_Onprem_Sqlserver_properties_typeProperties_userName": {
						"value": "[parameters('ls_Onprem_Sqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}