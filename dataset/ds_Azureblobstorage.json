{
	"name": "ds_Azureblobstorage",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_AzureBlobstorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"TableName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().TableName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(substring(utcNow(),0 ,4 ),'/',substring(utcNow(),5 ,2 ),'/',string(add(int(substring(utcNow(),8 ,2 )),1)),'/',replace(dataset().TableName,'.csv',''))",
					"type": "Expression"
				},
				"container": "landing"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}